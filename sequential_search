        "data = [20, 31, 50, 17, 16, 36, 19, 8]\n",
        "\n",
        "def sequential_search(data, key):\n",
        "  for i in data:\n",
        "    if i == key :\n",
        "      print(\"找到了\")\n",
        "      print(\"在\"+i+1)\n",
        "      break\n",
        "    elif i+1 == len(data):\n",
        "      print(\"沒找到\")\n",
        "      break\n",
        "x = 0\n",
        "input(x)\n",
        "SequentialSearch(data,x)\n",
        "\n",
        "\n",
        "\n",
        "data = [1,22,32,48,56,63,47,82,94]\n",
        "\n",
        "def binary_search(data, key):\n",
        "  #設置選取範圍的指標\n",
        "  low = 0\n",
        "  upper = len(data) - 1\n",
        "  whole low <= upper:\n",
        "  mid = int((low + upper) / 2) #取中間所引的值\n",
        "  if data[mid] < key: \n",
        "    low = mid + 1\n",
        "  elif data[mid] > key:\n",
        "    upper = mid - 1\n",
        "    else:\n",
        "       return mid\n",
        "  return -1\n",
        "\n",
        "\n",
        "\n",
        "index = binary_search(data,32)\n",
        "if index >= 0:\n",
        "  print(\"找到數值於所引\" + str(index))\n",
        "else:\n",
        "  print(\"找不到數值\")\n",
        "\n",
        "\n",
        "\n"
