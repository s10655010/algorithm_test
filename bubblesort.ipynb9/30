        "def Bubble_Sort(datas):\n",
        "    for i in range(1, len(datas)): # \n",
        "        for j in range(len(datas)-i):   # \n",
        "            if datas[j] > datas[j+1]:   # \n",
        "                datas[j], datas[j+1] = datas[j+1], datas[j]\n",
        "    return datas\n",
        "\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    import random\n",
        "    X = 0\n",
        "    b = {} #宣告矩陣\n",
        "    for x in range(0,6): #迴圈輸入\n",
        "      b.append(input()) #\n",
        "    random.shuffle(a)\n",
        "    print(b)\n",
        "    a = Bubble_Sort(b)\n",
        "    print(b)\n",
